    "babel-eslint": "7.0.0",
    "chai": "3.5.0",
    "eslint": "3.7.0",
    "eslint-config-airbnb-es5": "1.1.0",
    "eslint-loader": "1.5.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jasmine": "1.8.1",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-mocha": "4.6.0",
    "eslint-plugin-node": "4.2.2",
    "eslint-plugin-react": "6.3.0",
    "mocha": "2.5.1",
    "nodemon": "1.11.0",
    "nyc": "10.2.0",
    "should": "^7.0.2",
    "supertest": "^1.0.1"
  }
