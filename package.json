  "private": true,
  "engine-strict": true,
  "engines": {
    "node": "6.9.1"
  },
  "sasslintConfig": "./.sasslint.yml",
  "scripts": {
    "lint": "eslint api",
    "clear:all": "npm-run-all clear:node_modules",
    "clear:node_modules": "rm -rf node_modules",
    "prestart": "npm-run-all -ln db:wait db:migrate",
    "dev": "nodemon server.js",
    "db:wait": "wait-for-db",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "coverage": "nyc --reporter=html npm-run-all test",
    "test": "npm-run-all test:api",
    "test:api": "mocha 'api/**/*.{e2e,spec}.js'",
    "initialize-addresses": "./initialize-addresses.sh"
  },
  "dependencies": {
    "ansi-regex": "2.1.1",
    "bluebird": "^2.9.34",
    "body-parser": "~1.13.2",
    "bookshelf": "0.10.3",
    "compression": "1.6.1",
    "cors": "2.7.1",
    "debug": "~4.1.1",
    "express": "4.13.4",
    "express-healthcheck": "0.1.0",
    "extend": "^3.0.0",
    "geojsonhint": "^1.1.0",
    "has": "1.0.1",
    "jsonwebtoken": "5.5.4",
    "knex": "0.12.9",
    "knex-postgis": "0.2.1",
    "lodash": "^3.10.1",
    "mocha": "3.2.0",
    "morgan": "~1.6.1",
    "npm-run-all": "4.0.2",
    "pg": "6.1.5",
    "strip-bom": "3.0.0"
  },
